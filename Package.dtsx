              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH ProductSalesData AS ( 
 
    SELECT    
        SOD.ProductID,  
        P.Name AS ProductName,
		SOD.SalesOrderID,
        CASE   
            WHEN SOH.OnlineOrderFlag = 1 THEN 'Online'   
            ELSE 'In-Store'   
        END AS ChannelType,  
        COALESCE(SO.SpecialOfferID, 0) AS PromotionID,  
        DATEPART(YEAR, SOH.OrderDate) AS SalesYear, 
        SOH.TerritoryID,  
        SOD.OrderQty AS TotalQuantity,  
        SOD.LineTotal AS TotalSales,  
        TH.ActualCost AS TotalCost,  
       SOD.LineTotal -TH.ActualCost AS TotalProfit,  
  SOH.TotalDue AS TotalDue,
        CASE WHEN SO.SpecialOfferID &lt;&gt; 1 THEN SOD.LineTotal ELSE 0 END AS PromotedSales, 
        CASE WHEN SO.SpecialOfferID = 1 THEN SOD.LineTotal ELSE 0 END AS NonPromotedSales, 
        SOD.LineTotal / SOD.OrderQty AS RevenuePerUnit,
        PSC.ProductSubcategoryID, 
        PC.ProductCategoryID,
  CONVERT(VARCHAR(8), SOH.OrderDate, 112) AS DateKey
  
    FROM   
        Sales.SalesOrderDetail AS SOD  
    INNER JOIN   
        Sales.SalesOrderHeader AS SOH ON SOD.SalesOrderID = SOH.SalesOrderID  
    INNER JOIN   
        Production.Product AS P ON SOD.ProductID = P.ProductID  
    INNER JOIN   
        Production.TransactionHistory AS TH ON SOD.SalesOrderID = TH.ReferenceOrderID AND SOD.ProductID = TH.ProductID 
    LEFT JOIN   
        Sales.SpecialOffer AS SO ON SOD.SpecialOfferID = SO.SpecialOfferID  
    LEFT JOIN   
        Production.ProductSubcategory AS PSC ON P.ProductSubcategoryID = PSC.ProductSubcategoryID  
    LEFT JOIN   
        Production.ProductCategory AS PC ON PSC.ProductCategoryID = PC.ProductCategoryID)
 
SELECT 
    PSD.ProductID, 
    PSD.ProductName,
	PSD.SalesOrderID,
 PSD.ProductCategoryID,
 PSD.ProductSubcategoryID, 
    PSD.ChannelType, 
    PSD.PromotionID, 
    PSD.SalesYear,
    PSD.TerritoryID,
 PSD.DateKey,
    PSD.TotalQuantity, 
    PSD.TotalSales, 
    PSD.TotalCost, 
    PSD.TotalProfit, 
    PSD.RevenuePerUnit, 
    PSD.TotalDue, 
    PSD.PromotedSales, 
    PSD.NonPromotedSales, 
    RANK() OVER (ORDER BY PSD.TotalSales DESC) AS ProductRank
FROM  
    ProductSalesData AS PSD
ORDER BY PSD.ProductID,PSD.SalesOrderID, DateKey,TerritoryID;</property>
