              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT 
    CONVERT(VARCHAR(8), SOH.OrderDate, 112) AS DateKey,  -- Converts the date to YYYYMMDD format
    SOH.OrderDate AS Date,                                           
    DATENAME(WEEKDAY, SOH.OrderDate) AS DayOfWeek,                   
    DATENAME(MONTH, SOH.OrderDate) AS Month,                         
    'Q' + CAST(DATEPART(QUARTER, SOH.OrderDate) AS NVARCHAR(1)) AS Quarter,  
    YEAR(SOH.OrderDate) AS Year,                                      
    CASE 
        WHEN 
            SOH.OrderDate IN (
                '2011-01-01', '2012-01-01', '2013-01-01', '2014-01-01',
                '2011-12-25', '2012-12-25', '2013-12-25', '2014-12-25',
                '2011-07-04', '2012-07-04', '2013-07-04', '2014-07-04',
                '2011-11-24', '2012-11-22', '2013-11-28', '2014-11-27',
                '2011-11-25', '2012-11-23', '2013-11-29', '2014-11-28',
                '2011-04-24', '2012-04-08', '2013-03-31', '2014-04-20',
                '2011-02-14', '2012-02-14', '2013-02-14', '2014-02-14'
            ) 
        THEN 1
        ELSE 0 
    END AS Holiday                                                    
FROM 
    Sales.SalesOrderHeader AS SOH
GROUP BY 
    SOH.OrderDate, SOH.SalesOrderID</property>
